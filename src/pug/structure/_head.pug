doctype html
html(lang='en')
    head
        meta(charset="UTF-8")
        if data.title
            title= data.title
        meta(name="viewport" content="width=device-width, user-scalable=yes")
        block head
    body
        block style
        if data.styles
            each style in data.styles
                link(href=style rel="stylesheet")
                p   test
        block body